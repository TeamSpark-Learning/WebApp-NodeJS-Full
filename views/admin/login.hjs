{{<layout}}
	{{$content}}
		<div>
			<form method="post">
				<div>
					<label>Login: </label>
					<input type="text" id="tbLogin" name="tbLogin" />
				</div>
				<div>
					<label>Password: </label>
					<input type="password" id="tbPassword" name="tbPassword" />
				</div>
				<div>
					<button type="submit">Login</button>
				</div>
			</form>
		</div>
	{{/content}}
{{/layout}}