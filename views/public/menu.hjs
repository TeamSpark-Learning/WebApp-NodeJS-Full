{{<layout}}
	{{$content}}
		<h1>{{ title }}</h1>
		<h2>Our menu:</h2>
		{{#dishes}}
			<div>
				<h3>{{ title }}</h3>
				{{#description}}
					<p>{{ description }}</p>
				{{/description}}
				<p>{{ price }} USD</p>
				{{#imageUrl}}
					<div><img src="{{ imageUrl }}" /></div>
				{{/imageUrl}}
			</div>
		{{/dishes}}
	{{/content}}
{{/layout}}
